{require_otp_vsn, "R14B04|R15B02|R15B03"}.

{deps, [
    {leo_mq, ".*", {git, "https://github.com/leo-project/leo_mq.git", "0.12.7"}}
]}.

{erl_opts, [{d, 'NOTEST'},
            warn_obsolete_guard,
            warn_unused_import,
            warnings_as_errors,
            warn_shadow_vars,
            warn_export_vars,
            warn_export_all]}.
{xref_checks, [undefined_function_calls]}.
{cover_enabled, true}.
{clean_files, []}.
